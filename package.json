{
  "name": "gatsby-source-steam",
  "version": "0.0.1",
  "description": "Gatsby source plugin for building websites using Steam as a data source",
  "dependencies": {
    "gatsby-node-helpers": "^0.1.3",
    "isomorphic-fetch": "^2.2.1"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "jest": "^22.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesdanylik/gatsby-source-steam.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": "James Danylik <james.danylik@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesdanylik/gatsby-source-steam/issues"
  },
  "homepage": "https://github.com/jamesdanylik/gatsby-source-steam#readme"
}
